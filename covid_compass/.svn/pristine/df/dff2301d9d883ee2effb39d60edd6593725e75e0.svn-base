<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Menu-Mapper">

	<select id="test" resultType="com.spring.compass.vo.MenuVO">
		SELECT *
		FROM MENU
	</select>
	
	<select id="selectMenuByMenuInnb" parameterType="java.lang.String" 
			resultType="com.spring.compass.vo.MenuVO">
SELECT
    MENU_INNB       ,MENU_URL       ,MENU_ICON
   ,UPMENU_INNB     ,MENU_ISNAV     ,MENU_LEVEL
   ,MENU_NM
FROM
    MENU
WHERE MENU_INNB = #{menuInnb}
	</select>
	
	<select id="selectMenuList"
			resultType="com.spring.compass.vo.MenuVO">
SELECT
    MENU_INNB       ,MENU_URL       ,MENU_ICON
   ,UPMENU_INNB     ,MENU_ISNAV     ,MENU_LEVEL
   ,MENU_NM
FROM
    MENU
WHERE
	MENU_ISNAV = 1
	</select>
	
	<select id="selectSubMenuList" parameterType="java.lang.String"
			resultType="com.spring.compass.vo.MenuVO">
SELECT
    MENU_INNB       ,MENU_URL       ,MENU_ICON
   ,UPMENU_INNB     ,MENU_ISNAV     ,MENU_LEVEL
   ,MENU_NM
FROM
    MENU
WHERE
	MENU_ISNAV = 1
AND
	UPMENU_INNB = #{menuInnb}
	</select>
	
</mapper>  


